<resources xmlns:tools="http://schemas.android.com/tools">




    <style name="wheel">
        <item name="awv_dividerTextColor">@color/color_e8e8e8</item>
        <item name="awv_isLoop">false</item>
        <item name="awv_itemsVisibleCount">7</item>
        <item name="awv_lineSpace">2</item>
        <item name="awv_textsize">17</item>
    </style>

    <!-- tip progressbar end -->
    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/color_000000</item>
    </style>
    <style name="dialogstyle" parent="android:Animation">
        <!-- 指定显示的动画xml  -->
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <!-- 指定消失的动画xml  -->
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>

    </style>


</resources>